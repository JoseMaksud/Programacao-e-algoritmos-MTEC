Algoritmo "Maior"
//Verificar entre dois valores qual é o maior

Var
   A,B:inteiro


Inicio
      escreval("Digite 2 valores")
      leia(A,B)
      se (A>B) entao
      escreval(A," é maior que",B)
      senao
      se (B>A) entao
      escreval(B," é maior que",A)
      senao
      escreval("São iguais")
      fimse
      fimse


Fimalgoritmo